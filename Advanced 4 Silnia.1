#include <stdio.h>

unsigned long int silnia(unsigned long int s){
  if (s < 2) return 1;

  return s * silnia(s - 1);
}

int main()
{
  int n = 5;
  int k = 2;

  printf("Silnia wynosi: %lu\n", silnia(4) + silnia(6) - silnia(2));

  printf("Wartość symbolu Newtona: %lu", silnia(n)/(k*silnia(n-k)));

  return 0;
}
